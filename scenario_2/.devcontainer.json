{
	"name": "Debug Sample",
	"dockerFile": "./Dockerfile",
	"extensions": [
		"ms-python.python"
	],
	"shutdownAction": "stopContainer",
	"workspaceFolder": "/",
	"overrideCommand": true,
	"postCreateCommand": "/usr/local/bin/python -m pip install -U pylint --user"
}